{
  "name": "chronos-fe",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "generate-config": "node generate-config.js",
    "start": "node server.js",
    "publish": "pnpm generate-config && pnpm build && pnpm start",
