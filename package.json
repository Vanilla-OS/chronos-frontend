    "fastify": "^5.3.3",
    "highlight.js": "^11.11.1",
    "js-yaml": "^4.1.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.3",
    "unhead": "^1.11.18",
    "vue": "^3.5.10",
    "vue-highlightjs": "^1.3.3",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
